{
  "name": "jelly-verse-offchain-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "node-run": "node --experimental-specifier-resolution=node --loader ts-node/esm --no-warnings=ExperimentalWarning",
    "typeorm": "yarn node-run node_modules/.bin/typeorm",
    "typeorm:d": "yarn node-run node_modules/.bin/typeorm -d src/db/datasource.ts",
    "typeorm:schema:sync": "yarn typeorm:d schema:sync",
    "typeorm:migration:generate": "yarn typeorm:d migration:generate",
    "typeorm:migration:show": "yarn typeorm:d migration:show",
    "typeorm:migration:run": "yarn typeorm:d migration:run",
    "generate:resource": "nest g resource ./modules/$npm_config_name",
    "generate:module": "nest g module ./modules/$npm_config_name",
    "codegen": "graphql-codegen --config src/gql/codegen.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.509.0",
    "@aws-sdk/credential-providers": "^3.509.0",
    "@helia/dag-json": "^1.0.3",
    "@helia/json": "^1.0.3",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/typeorm": "^9.0.1",
    "@openzeppelin/merkle-tree": "^1.0.5",
    "@pinata/sdk": "^2.1.0",
    "axios": "^1.6.2",
    "bignumber.js": "^9.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ethers": "5.7",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "joi": "^17.7.0",
    "nodemailer": "^6.9.4",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.4.2",
    "@golevelup/ts-jest": "^0.3.7",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.6",
    "@types/node": "18.15.10",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.9",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "jest-runner-groups": "^2.2.0",
    "lint-staged": "^13.1.1",
    "prettier": "^3.0.3",
    "rimraf": "5.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "swagger-typescript-api": "^13.0.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
